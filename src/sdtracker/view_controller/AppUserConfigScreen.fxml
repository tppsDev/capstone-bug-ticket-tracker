<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="592.0" prefWidth="800.0" styleClass="white-background" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="sdtracker.view_controller.AppUserConfigScreenController">
    <stylesheets>
        <URL value="@main.css" />
    </stylesheets>
   <children>
      <AnchorPane prefHeight="80.0" prefWidth="800.0" styleClass="screen-header">
         <children>
            <Label fx:id="titleLabel" layoutY="14.0" prefHeight="44.0" prefWidth="800.0" styleClass="pane-header-title" text="User Configuration" />
            <ProgressBar fx:id="progressIndicator" layoutX="300.0" layoutY="57.0" prefWidth="200.0" />
         </children>
      </AnchorPane>
      <Button fx:id="addAppUserButton" layoutX="14.0" layoutY="91.0" mnemonicParsing="false" onAction="#handleAddAppUserButton" styleClass="form-button" text="Add User" />
      <Label layoutX="23.0" layoutY="141.0" prefHeight="18.0" prefWidth="73.0" styleClass="form-label" text="Search:" />
      <TextField fx:id="searchNameTextField" layoutX="106.0" layoutY="136.0" prefHeight="29.0" prefWidth="244.0" styleClass="form-text-field" />
      <TableView fx:id="appUserTableView" layoutX="15.0" layoutY="172.0" prefHeight="354.0" prefWidth="772.0">
         <columns>
            <TableColumn fx:id="deleteColumn" editable="false" prefWidth="40.0" />
            <TableColumn fx:id="editColumn" editable="false" prefWidth="40.0" />
            <TableColumn fx:id="nameColumn" editable="false" prefWidth="300.0" text="Name" />
            <TableColumn fx:id="usernameColumn" editable="false" prefWidth="150.0" text="Username" />
            <TableColumn fx:id="departmentColumn" editable="false" prefWidth="240.0" text="Department" />
         </columns>
      </TableView>
      <Label fx:id="systemMessageLabel" layoutX="15.0" layoutY="536.0" prefHeight="35.0" prefWidth="772.0" styleClass="system-message-label" wrapText="true" />
      <TextField fx:id="searchUsernameTextField" layoutX="400.0" layoutY="136.0" prefHeight="29.0" prefWidth="131.0" styleClass="form-text-field" />
      <ComboBox fx:id="departmentFilterComboBox" layoutX="555.0" layoutY="136.0" prefHeight="29.0" prefWidth="224.0" styleClass="form-text-field" />
   </children>
</AnchorPane>
